            ],
            "body": {
              "mode": "raw",
              "raw": "{\\n  \\\"firstName\\\": \\\"Admin\\\",\\n  \\\"lastName\\\": \\\"User\\\",\\n  \\\"email\\\": \\\"admin@example.com\\\",\\n  \\\"password\\\": \\\"password123\\\",\\n  \\\"role\\\": \\\"ADMIN\\\"\\n}"
